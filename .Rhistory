hurricane_computation <- function(aoi,
classified_raster) {
mask <- mask(classified_raster, aoi)
area_raster = cellSize(r_mask, unit="km")
patchsizes <- zonal(
area_raster, r_mask, sum)
class_name <- data.frame(max = c(0, 1),
classes = c("not_affected", "affected"))
out <- merge(
x = patchsizes, y = class_name,
by.x = colnames(patchsizes)[1],
by.y = colnames(class_name)[1])
result <- out[,-1]
result
}
